@using BTL_TTNhom.Models.Entity;
@model List<NHAXUATBAN>
<section class="partner">
    <div class="container" style="background-color: white;">
        <div class="row" style="padding: 10px 0 30px 0;">
            <div class="col-lg-12 col-12">
                <h3 class="title" style="margin-bottom: 0px;">
                    <span>Đối tác</span>
                </h3>
                <div id="owl-carous" class="partner-body owl-carousel">
                    @foreach (var item in Model)
                    {
                        <div class="item">
                            <a href="#">
                                <img src="~/@item.LINKANH" alt="#" style="width: 150px; height: auto;">
                            </a>
                        </div>
                    }                  
                </div>
            </div>
        </div>
    </div>
</section>
